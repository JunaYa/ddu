{
  "$schema": "https://schema.tauri.app/config/2.0.0",
  "productName": "ddu",
  "version": "0.1.0",
  "identifier": "com.ddu.app",
  "build": {
    "beforeDevCommand": "pnpm dev",
    "beforeBuildCommand": "pnpm generate",
    "devUrl": "http://localhost:3000",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "setting",
        "label": "setting",
        "url": "/setting",
        "width": 600,
        "height": 400,
        "fullscreen": false,
        "minimizable": false,
        "maximizable": false,
        "visible": false
    }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
